# =============================================================================
# API ENVIRONMENT VARIABLES
# =============================================================================
# Copy this file to .env and update with your values

# Database
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@localhost:5433/uni-feedback

# Environment
NODE_ENV=development

# API Server
PORT=3001
DASHBOARD_URL=localhost:5174
WEBSITE_URL=http://localhost:3000

# CORS - Comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:5174,https://uni-feedback.com,https://ssr.uni-feedback.com,https://admin.uni-feedback.com

RESEND_API_KEY=
POSTHOG_API_KEY=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Feature Flags
# Validate that submitted feedback emails match the faculty's allowed email suffixes
VALIDATE_EMAIL_SUFFIX=false