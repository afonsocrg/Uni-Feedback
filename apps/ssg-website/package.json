{
  "name": "@uni-feedback/ssg-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx react-router build",
    "cf-typegen": "wrangler types",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "npm run build && wrangler deploy",
    "dev": "npx react-router dev -l info",
    "dev:ssg-website": "npx react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run cf-typegen && npx react-router typegen && tsc -b"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.81.2",
    "@uni-feedback/database": "file:../../packages/database",
    "drizzle-orm": "^0.38.4",
    "framer-motion": "^11.18.2",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.1",
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.3",
    "drizzle-kit": "^0.30.6",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.33.1"
  },
  "overrides": {}
}
